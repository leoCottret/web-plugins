{
    "manifest_version": 2,
    "name": "Radio France Ultimate",
    "version": "1.2",
    "description": "Add shortcuts for the Radio France player",
    "content_scripts": [
        {
            "matches": ["*://*.radiofrance.fr/*"],
            "js": ["radio_france_ultimate.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "*://*.radiofrance.fr/*"
    ]
}
